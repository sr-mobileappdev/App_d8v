{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "eslint:fix": "eslint '{components,containers,screens,store,src}/**/*.js' --fix",
    "test": "jest",
    "debug-ios": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "debug-windows": "rndebugger-open",
    "android": "yarn android-all",
    "android-all": "concurrently \"npm run android:dubugger\" \"npm run android:packager\" \"cd android && ./gradlew installDebug && cd ..\" \"adb reverse tcp:8081 tcp:8081\"",
    "android:packager": "node node_modules/react-native/local-cli/cli.js start --port 8082",
    "android:dubugger": "open 'rndebugger://set-debugger-loc?port=8082'",
    "android-build-prod": "export ENVFILE=.env && cd android && ./gradlew assembleRelease && cd .."
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@mapbox/react-native-mapbox-gl": "^6.1.3",
    "@nart/react-native-swiper": "^1.5.13",
    "autobind-decorator": "^2.4.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.21",
    "lottie-react-native": "^2.5.11",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-android-open-settings": "^1.2.0",
    "react-native-autocomplete-input": "^3.6.0",
    "react-native-blur": "^3.2.2",
    "react-native-camera": "^1.6.3",
    "react-native-config": "^0.11.7",
    "react-native-deck-swiper": "^1.5.22",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-grid-list": "^1.0.9",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.4.2",
    "react-native-loader": "^1.2.1",
    "react-native-masonry-layout": "^1.0.6",
    "react-native-modal": "^7.0.2",
    "react-native-modal-datetime-picker": "^6.0.0",
    "react-native-parallax-scroll-view": "^0.21.3",
    "react-native-permissions": "^1.1.1",
    "react-native-picker-dropdown": "^0.1.2",
    "react-native-scroll-into-view": "^0.1.4",
    "react-native-sideswipe": "^1.4.2",
    "react-native-simple-toast": "^0.0.8",
    "react-native-slider": "^0.11.0",
    "react-native-snap-carousel": "^3.7.4",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^8.0.10",
    "react-native-tab-view": "^1.3.0",
    "react-native-text-size": "^2.0.4",
    "react-native-tooltips": "^0.0.10",
    "react-native-vector-icons": "^6.1.0",
    "react-navigation": "^3.0.8",
    "react-redux": "^6.0.0",
    "react-test-renderer": "16.6.3",
    "react-timer-mixin": "^0.13.4",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "redux-actions": "2.6.4",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "3.0.6",
    "redux-persist": "5.10.0",
    "redux-saga": "0.16.2",
    "redux-thunk": "2.3.0",
    "remote-redux-devtools": "0.5.13",
    "reselect": "4.0.0",
    "standard": "^12.0.1",
    "styled-components": "^4.1.2",
    "styled-is": "^1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.2.2",
    "@babel/plugin-syntax-export-namespace-from": "7.0.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.9.4",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-preset-env": "2.0.0-alpha.20",
    "babel-preset-jest": "23.2.0",
    "babel-preset-react-native": "5.0.2",
    "babel-preset-react-native-stage-0": "1.0.1",
    "enzyme": "3.7.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-native": "3.5.0",
    "flow-bin": "0.86.0",
    "flow-typed": "2.5.1",
    "jest": "23.6.0",
    "jest-enzyme": "7.0.1",
    "jest-repl": "23.6.0",
    "lint-staged": "^7.3.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3",
    "schedule": "0.4.0"
  },
  "jest": {
    "preset": "react-native",
    "bail": true,
    "verbose": true,
    "automock": false,
    "cacheDirectory": "<rootDir>/.jest-cache",
    "moduleNameMapper": {
      "^React$": "<rootDir>/node_modules/react"
    },
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
